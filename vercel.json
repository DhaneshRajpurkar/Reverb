{
  "version": 2,
  "name": "reverb",
  "env": {
    "FLASK_ENV": "production"
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "env": {
        "FLASK_ENV": "production"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
